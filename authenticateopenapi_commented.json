"/authenticate": {
    "post": {
      "tags": [
        "Authenticate"
      ],
      "requestBody": {
        "required": true,
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/CredentialsParams"
            }
          }
        }
      },
      "responses": {
        "200": {
          "description": "Successfully login and token generated.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GenerateTokenSuccessOutputParams"
              }
            }
          }
        },
        "400": {
          "description": "Some issue occured while login",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/StorageGridFailureOutputParams"
              }
            }
          }
        },
        "403": {
          "description": "You do not have necessary permissions for the resource"
        }
      }
    }
  },