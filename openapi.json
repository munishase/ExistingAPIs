    "/processstoragegrid": {
      "post": {
        "tags":["Storagegrid"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/StorageGridInputParams"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "New Storagegrid account(tenant account, key creation and bucket creation) has been configured.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/StorageGridSuccessOutputParams"
                }
              }
            }
          },
          "400": {
            "description": "Some issue occured while configuring Storagegrid",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/StorageGridFailureOutputParams"
                }
              }
            }
          },
          "403": {
            "description": "You do not have necessary permissions for the resource"
          }
        }
      }
    },
